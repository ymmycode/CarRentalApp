	<queryString language="SQL">
		<![CDATA[SELECT
rental.idRental,
pelanggan.nama,
mobil.merek,
mobil.nama,
mobil.jenis,
mobil.plat,
rental.jangkaWaktu,
rental.tglPinjam,
rental.tglKembali,
pembayaran.total,
pembayaran.uangmuka,
pembayaran.sisabayar,
pembayaran.`status`
FROM
rental
INNER JOIN pelanggan ON rental.idPelanggan = pelanggan.idPelanggan
INNER JOIN mobil ON rental.idMobil = mobil.idMobil
INNER JOIN pembayaran ON rental.idRental = pembayaran.idRental
WHERE
rental.idRental = 1]]>
	</queryString>
