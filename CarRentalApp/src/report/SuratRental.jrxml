	<queryString language="SQL">
		<![CDATA[SELECT
rental.idRental,
pelanggan.nama,
pelanggan.ktp,
pelanggan.alamat,
pelanggan.telp,
mobil.merek,
mobil.nama AS nama_mobil,
mobil.jenis,
mobil.plat,
rental.jangkaWaktu,
rental.tglPinjam,
rental.tglKembali,
pembayaran.total,
pembayaran.uangmuka
FROM
rental
INNER JOIN pelanggan ON rental.idPelanggan = pelanggan.idPelanggan
INNER JOIN mobil ON rental.idMobil = mobil.idMobil
INNER JOIN pembayaran ON rental.idRental = pembayaran.idRental
WHERE
rental.idRental = 1]]>
	</queryString>
